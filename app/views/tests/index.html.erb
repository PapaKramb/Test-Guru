<h1>Tests</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Level:</th>
      <th colspan = 3>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @tests.each do |test| %>
      <tr>
        <td><%= test.title %></td>
        <td><%= test.level %></td>
        <td><%= link_to 'Show', test %></td>
        <td><%= link_to 'Edit', edit_test_path(test) %></td>
        <td><%= link_to 'Delete', test, method: :delete, data: { confirm: 'You are sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<p>
  <%= link_to 'Create new test', new_test_path %>
</p>