<p>
  <%= link_to t('.new_test'), new_admin_test_path, class: 'btn btn-outline-secondary btn-lg' %>
</p>

<p>
  <%= link_to t('.new_badge'), admin_badges_path, class: 'btn btn-outline-secondary btn-lg' %>
</p>

<p>
  <%= link_to "Gists", admin_gists_path, class: 'btn btn-outline-secondary btn-lg' %>
</p>

<%= render 'shared/errors', resource: @test %>

<div class="row mt-3">
  <div class="col col-md">
    <h1 class="display-4 text-center"><%= t('.header') %></h1>
    <table class="table text-center mt-3">
      <thead>
        <tr>
          <th scope="col" class="sort-by-title pointer">
            <%= t('.title') %>
            <%= octicon 'arrow-up', class: 'text-success hide' %>
            <%= octicon 'arrow-down', class: 'text-success hide' %>
          </th>
          <th scope="col"><%= t('.level') %></th>
          <th scope="col"><%= t('.category') %></th>
          <th scope="col"><%= t('.questions') %></th>
          <th scope="col"><%= t('.actions') %></th>
        </tr>
      </thead>
      <tbody>
        <%= render @tests %>
      </tbody>
    </table>
  </div>
</div>